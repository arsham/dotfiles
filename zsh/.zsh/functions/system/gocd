# Quick change to go project

local targetlist=$(find $HOME/Projects/ -type d -not -path '*/\.*' -not -path '*\/pkg\/mod\/*' -not -path '*\/vendor\/*')

if [[ -z "$targetlist" ]]
then
    echo "Could not find any projects"
    return
fi

if [[ "${@:1}" ]]
then
    cd "$(echo $targetlist | fzf +m --query ${1:-})"
else
    cd "$(echo $targetlist | fzf +m)"
fi
