local targetlist=$(find $HOME/Projects/ -type d -not -path '*/\.*' -exec realpath {} +)

if [[ -z "$targetlist" ]]
then
    echo "Could not find any projects"
    return
fi
if [[ "${@:1}" ]]
then
    local query=${@:1}
    cd $(echo $targetlist | fzf --preview 'ls {}' --query $query)
    return
else
    cd $(echo $targetlist | fzf --preview 'ls {}')
fi
